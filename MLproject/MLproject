name: latihan-cs-mlflow

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: { type: int, default: 100 }
      max_depth: { type: int, default: 15 }
      min_samples_split: { type: int, default: 10 }
      min_samples_leaf: { type: int, default: 5 }
      dataset: { type: string, default: "train_pca.csv" }
    command: "python modelling.py {n_estimators} {max_depth} {min_samples_split} {min_samples_leaf} {dataset}"
